{
	"name": "ppl-debugger",
	"description": "PPL Debugger",
	"version": "1.0.0",
	"publisher": "Anonymous Author(s)",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/anonymous_authors/ppl-debugger"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onWebviewPanel:pplDebugger"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pplDebugger.start",
				"title": "Start Debugging File",
				"category": "PPL Debugger"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"json-rpc-2.0": "^1.7.0",
		"fft.js": "^4.0.4"
	},
	"devDependencies": {
		"@types/node": "^18",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.26.0",
		"typescript": "^5.4.2"
	}
}
